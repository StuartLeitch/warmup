      <add ext="pack"/>
      <add ext="suo"/>
      <add ext="ico"/>
      <add ext="nupkg"/>
      <add ext="zip"/>
      <add ext="7z"/>
    </ignoredExtensions>
  </warmup>

<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
